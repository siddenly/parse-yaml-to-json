    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "esmodules": true
                }
            }
        ]
    ],
    "plugins": [
        ["@babel/plugin-proposal-pipeline-operator", { "proposal": "minimal" }]
    ],
    "sourceMaps": true,
    "retainLines": true
}